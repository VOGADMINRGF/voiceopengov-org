{
  "name": "@vog/web",
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate --schema ../../prisma/web/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema ../../prisma/web/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema ../../prisma/web/schema.prisma",
    "idx:core": "tsx scripts/core.ensureIndexes.ts",
    "idx:votes": "tsx scripts/votes.ensureIndexes.ts",
    "idx:all": "pnpm run idx:core && pnpm run idx:votes"
  },
  "dependencies": {
    "next": "15.3.2",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  }
}
