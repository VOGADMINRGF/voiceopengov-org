{
  "name": "@vog/web",
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": ">=20.x"
  },
  "scripts": {
    "prisma:generate": "node ./scripts/prisma-generate.mjs",
    "build": "pnpm --filter @vog/ui run build && pnpm run prisma:generate && next build",
    "dev": "pnpm --filter @vog/ui run build && next dev",
    "start": "next start -p 3000",
    "lint": "next lint",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@vog/core": "workspace:*",
    "@vog/features": "workspace:*",
    "@vog/ui": "workspace:*",
    "bcryptjs": "^3.0.2",
    "framer-motion": "^12.23.22",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.18.3",
    "neo4j-driver": "^5.28.2",
    "next": "15.3.2",
    "next-auth": "^4.24.11",
    "otplib": "^12.0.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "^4.12.0",
    "redis": "^5.8.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^22.7.5",
    "eslint": "^9.11.1",
    "eslint-config-next": "15.3.2",
    "typescript": "^5.9.2"
  }
}
