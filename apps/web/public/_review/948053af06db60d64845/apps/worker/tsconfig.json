{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["../../*"],
      "@core/*": ["../../core/*"],
      "@/core/*": ["../../core/*"],
      "@prisma/*": ["../../prisma/*"],
      "@/prisma/*": ["../../prisma/*"]
    },
    "types": ["node"]
  },
  "include": ["src", "../../core", "../../prisma"]
}
