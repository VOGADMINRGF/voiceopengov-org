{
  "name": "@vog/worker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
     "postinstall": "node -e \"const fs=require('fs'),cp=require('child_process'),p=require('path');const f=p.resolve(__dirname,'../../scripts/optional-prisma.js');if(fs.existsSync(f)){cp.spawnSync(process.execPath,[f],{stdio:'inherit'});}else{console.log('[worker] optional-prisma not found, skipping:',f);}\"",
    "dev": "tsx -r tsconfig-paths/register src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node --env-file=.env dist/index.js"
  },
  "dependencies": {
    "bullmq": "^5.7.0",
    "ioredis": "^5.4.1"
  },
  "devDependencies": {
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}
