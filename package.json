{
  "name": "vpm25",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "workspaces": [
    "apps/landing",
    "apps/web"
  ],
  "scripts": {
    "dev:landing": "pnpm --filter @vog/landing... dev",
