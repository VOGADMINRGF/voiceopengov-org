+function ensureImpactAndResponsibility(
+  value: AnalyzeResult["impactAndResponsibility"] | undefined,
+): AnalyzeResult["impactAndResponsibility"] {
+  if (!value || typeof value !== "object") {
+    return { impacts: [], responsibleActors: [] };
+  }
+
+  const impacts = (value as any).impacts;
+  const responsibleActors = (value as any).responsibleActors;
+
+  return {
+    impacts: Array.isArray(impacts) ? impacts.slice(0, 12) : [],
+    responsibleActors: Array.isArray(responsibleActors) ? responsibleActors.slice(0, 12) : [],
+  };
+}
+
+function ensureReport(
+  value: AnalyzeResult["report"] | undefined,
+): AnalyzeResult["report"] {
+  if (!value || typeof value !== "object") {
+    return {
+      summary: null,
+      keyConflicts: [],
+      facts: { local: [], international: [] },
+      openQuestions: [],
+      takeaways: [],
+    };
+  }
+
+  const facts = (value as any).facts;
+  const keyConflicts = (value as any).keyConflicts;
+  const openQuestions = (value as any).openQuestions;
+  const takeaways = (value as any).takeaways;
+
+  return {
+    summary: typeof (value as any).summary === "string" ? (value as any).summary : null,
+    keyConflicts: Array.isArray(keyConflicts) ? keyConflicts.slice(0, 12) : [],
+    facts: {
+      local: facts && Array.isArray(facts.local) ? facts.local.slice(0, 12) : [],
+      international:
+        facts && Array.isArray(facts.international)
+          ? facts.international.slice(0, 12)
+          : [],
+    },
+    openQuestions: Array.isArray(openQuestions) ? openQuestions.slice(0, 12) : [],
+    takeaways: Array.isArray(takeaways) ? takeaways.slice(0, 12) : [],
+  };
+}
