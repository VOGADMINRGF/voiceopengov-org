{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "@core/*": [
        "core/*"
      ],
      "@features/*": [
        "features/*"
      ],
      "@/features/*": [
        "features/*"
      ],
      "@db-core": [
        "packages/db-core/src/client.ts"
      ],
      "@vog/ui": ["packages/ui/src/index.ts"],
"@vog/ui/*": ["packages/ui/src/*"],
"@ui": ["packages/ui/src/index.ts"],
"@ui/*": ["packages/ui/src/*"],
      "@db/web": [
        "./packages/db-web/src/index.ts"
      ],
      "@db/web/*": [
        "./packages/db-web/src/*"
      ],
      "@packages/*": [
        "packages/*"
      ],
      "@/*": [
        "apps/web/src/*"
      ]
    },
    "types": [
      "node"
    ],
    "verbatimModuleSyntax": false,
    "jsx": "react-jsx"
  },
    "exclude": [
      "apps/web/scripts/**",
      "**/__tests__/**",
      "**/*.test.ts",
      "**/*.test.tsx",
      "features_2/**",
      "_legacy_src/**",
      "**/*.bak",
      "**/_repair_backup_*/**",
      "apps/web/src/__features_local_DISABLED__/**"
    ]
}
